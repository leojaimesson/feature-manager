// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Project {
  id          String   @id
  name        String
  description String
  createdAt   DateTime
  updatedAt   DateTime
}

model Feature {
  id          String   @id
  name        String
  description String
  enable      Boolean
  createdAt   DateTime
  updatedAt   DateTime
}
